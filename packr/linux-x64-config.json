{
    "platform": "linux64",
    "jdk": "linux-jdk",
    "executable": "SpoonLite",
    "classpath": [
        "build/libs/SpoonLite-shaded.jar"
    ],
    "mainclass": "net.runelite.launcher.Launcher",
    "vmargs": [
       "-Drunelite.launcher.nojvm=true",
       "-Xmx512m",
       "-Xss2m",
       "-XX:CompileThreshold=1500"
    ],
    "output": "native-linux-x86_64/SpoonLite.AppDir/",
    "resources": [
      "build/filtered-resources/SpoonLite.desktop",
      "appimage/runelite.png"
    ]
}
